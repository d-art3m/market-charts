{
  "rewrites": [
    { "source": "/proxy/:path*", "destination": "https://platform.fintacharts.com/:path*" },
    { "source": "/api/:path*", "destination": "/api/:path*" },
    { "source": "/:path*.:ext*", "destination": "/:path*.:ext*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}