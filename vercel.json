{
  "rewrites": [
    { "source": "/proxy/:path*", "destination": "https://platform.fintacharts.com/:path*" },
    { "source": "/api/:path*", "destination": "/api/:path*" },
    { "source": "/:path*\\..*", "destination": "/:path*\\..*" }, // Исключение для файлов с расширением
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}